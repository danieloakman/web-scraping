{
  "name": "web-scraping",
  "version": "0.1.0",
  "description": "Web scraping",
  "type": "module",
  "scripts": {
    "build": "sst build --stage prod",
    "check": "tsc --noEmit",
    "cleaninstall": "rm -rf node_modules .sst",
    "deploy": "sst deploy --stage prod",
    "dev": "sst dev",
    "format": "prettier --write ./src && eslint --fix ./src",
    "lint": "prettier --check ./src && eslint ./src",
    "remove": "sst remove --stage prod"
  },
  "author": "Daniel Brown",
  "dependencies": {
    "@danoaky/js-utils": "^0.29.0",
    "@sparticuz/chromium": "^138",
    "http-status-codes": "^2.3.0",
    "iteragain": "^4.1.6",
    "playwright-core": "1.52",
    "sst": "^3.17.10",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@playwright/test": "1.52",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^24.1.0",
    "chromium": "^3.0.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}